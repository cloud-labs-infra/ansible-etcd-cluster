# {{ ansible_managed }}
[Unit]
Description=etcd service
Documentation=https://github.com/etcd-io/etcd
After=network.target

[Service] 
User={{ etcd_user }}
Group={{ etcd_group }}
Type=notify
ExecStart={{ etcd_bin_path }} --config-file {{ etcd_config_file }}
Restart=always
RestartSec=10s
LimitNOFILE=40000

[Install] 
WantedBy=multi-user.target
